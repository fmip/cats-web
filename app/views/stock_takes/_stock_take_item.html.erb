<%= fields_for(stock_take_item)  do |f| %>
<tr class="gradeX">
                <td><%= CommodityCategory.find(stock_take_item.commodity_category_id).name%></td>
                <td><%= Commodity.find(stock_take_item.commodity_id).name%></td>
                <td><%=stock_take_item.theoretical_amount%></td>
