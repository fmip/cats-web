<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-sm-4">
    <h2>Add Stock Item to Adjust</h2>
    <ol class="breadcrumb">
      <li>
        <a href = "#">Stock</a>
      </li>
      <li>
        <%= link_to 'Adjustments', stock_takes_path %>        
      </li>
      <li class="active">
        <strong>New</strong>
      </li>
    </ol>
  </div>
  <div class="col-sm-8">
    <div class="title-action">
      <%= link_to 'Cancel', stock_take_path(@stock_take),  :class => "btn btn-default"%>
    </div>
  </div>
</div>

<%= render 'form', stock_take_item: @stock_take_item %>

